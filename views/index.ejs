<form
  action="/uploadPhoto"
  enctype="multipart/form-data"
  method="post"
>
  <input
    class="choose-file-btn"
    type="file"
    name="myImage"
  />
  <input
    class="upload-btn"
    type="submit"
    value="Upload Photo"
  />
</form>

<div class="uploaded-images">
  <% images.forEach(function(image) { %>
    <img
      class="image"
      src="data:image/<%=image.image.contentType%>;base64,
    <%=image.image.data.toString('base64')%>">
  <% }); %>
</div>